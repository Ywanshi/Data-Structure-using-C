#include <stdio.h>
int main(){
    int arr[5];


    Array Insertion.
    printf("Enter the elements in array: \n");
    for(int i = 0; i<5; i++){
        scanf("%d", &arr[i]);
    }


    Output from an array.
    printf("\n The elements that you have enterd are: ");
    for(int i = 0; i<5; i++){
        printf("%d  ", arr[i]);
    }


    return 0;
}
